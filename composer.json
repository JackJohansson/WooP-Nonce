{
    "name": "johansson/woopnonce",
    "description": "A composer package to use WordPress nonces in an OOP way.",
    "type": "package",
	"keywords": [
		"nonce",
		"wordpress",
		"wordpress nonce"
	  ],
    "require": {
        "php": ">=5.6"
    },
	"require-dev": {
        "phpunit/phpunit": "3.7.14"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jack Johansson",
            "email": "jack.johansson@yahoo.com",
			"role":  "Developer"
        }
    ],
	"autoload": {
		"psr-4": {
			"WoopNonce\\": "src/"    
		},
		"files": [
			"src/WoopNonce.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
		  "WoopNonce\\Test\\": "test"
		}
	}
}
